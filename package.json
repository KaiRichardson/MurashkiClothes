{
	"name": "murashkiclothes",
	"version": "0.1.0",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/KaiRichardson/MurashkiClothes.git"
	},
	"author": "Zach Murphy/Kai Richardson",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/KaiRichardson/MurashkiClothes/issues"
	},
	"homepage": "https://murashki.herokuapp.com/",
	"scripts": {
		"start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
		"start:prod": "node server.js",
		"start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
		"client": "cd client && npm run start",
		"seed": "node scripts/seedDB.js",
		"install": "cd client && npm install",
		"build": "cd client && npm run build",
		"heroku-postbuild": "npm run build",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"axios": "^0.19.0",
		"bcrypt": "^3.0.6",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.7.10",
		"morgan": "^1.9.1"
	},
	"devDependencies": {
		"morgan": "^1.9.1",
		"nodemon": "^1.19.4",
		"@fortawesome/fontawesome-free": "^5.10.1",
		"concurrently": "^4.1.0",
		"prettier": "^1.17.1"
	}
}
